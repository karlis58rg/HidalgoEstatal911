<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/textToll"
    tools:context=".MensajeSalida">

    <ImageView
        android:id="@+id/imgHeaderCyHReporte"
        android:layout_width="wrap_content"
        android:layout_height="88dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/ic_barracasayh_911" />

    <ImageButton
        android:id="@+id/btnSalidaRegresar"
        android:layout_width="80dp"
        android:layout_height="88dp"
        android:background="@drawable/ic_btncasa_911" />

    <ImageView
        android:id="@+id/lblReporteEnviado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgHeaderCyHReporte"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/ic_reporteok"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"/>

    <ImageView
        android:id="@+id/lblReporteEnviadoPaloma"
        android:layout_below="@id/lblReporteEnviado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        app:srcCompat ="@drawable/ic_palomaverde_911"
        android:scaleType="fitXY"
        android:layout_centerHorizontal="true"/>

    <ImageView
        android:id="@+id/lblRecuadro"
        android:layout_below="@id/lblReporteEnviadoPaloma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/ic_recibidook"
        android:scaleType="fitXY"/>

    <TextView
        android:id="@+id/lblFolio"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/lblReporteEnviado"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="100dp"
        android:layout_marginRight="100dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="@color/txtColor"
        android:textSize="17dp" />

</RelativeLayout>